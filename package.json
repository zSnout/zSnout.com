{
  "name": "zsnout",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "npx vite",
    "build": "npx vue-tsc --noEmit && npx vite build && npm run build-server",
    "build-server": "npx esbuild ./server/index.ts --bundle --platform=node --target=node16 --minify --format=esm --external:bcrypt --external:mongodb --external:nodemailer --external:socket.io --external:./node_modules/* --outfile=./dist/server.mjs && sed -i 's+../node_modules/++g' ./dist/server.mjs",
    "preview": "npx vite preview"
  },
  "engines": {
    "node": "v16.14.2",
    "npm": "8.5.0"
  },
  "dependencies": {
    "@tiptap/extension-collaboration": "^2.0.0-beta.38",
    "@tiptap/starter-kit": "^2.0.0-beta.191",
    "@tiptap/vue-3": "^2.0.0-beta.96",
    "@vueuse/components": "^9.1.0",
    "@vueuse/core": "^8.9.4",
    "@vueuse/integrations": "^9.1.0",
    "@vueuse/shared": "^8.9.4",
    "bcrypt": "^5.0.1",
    "disable-scroll": "^0.6.0",
    "error-stack-parser": "^2.1.4",
    "fast-glob": "^3.2.11",
    "glsl-canvas-js": "^0.2.9",
    "hoodmane-pyodide": "^0.20.0-m",
    "mix-css-color": "^0.2.0",
    "mongodb": "^4.8.1",
    "random-seed": "^0.3.0",
    "rangetouch": "^2.0.1",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "storymatic": "^2.0.85",
    "tabbable": "^5.3.3",
