    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/nodemailer": "^6.4.4",
    "@types/random-seed": "^0.3.3",
    "@vitejs/plugin-vue": "^2.3.3",
    "esbuild": "^0.14.53",
    "nodemailer": "^6.7.7",
    "prettier-plugin-organize-attributes": "^0.0.5",
